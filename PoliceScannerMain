import java.util.*;
import java.io.*;
public class Main {

	public static void main(String[] args)throws IOException {
		File infill = new File("suspects.txt");
		
		Scanner s = new Scanner(System.in);
		
		
		
		MySalesSkel skel = new MySalesSkel();
		String key = s.nextLine().toUpperCase();
		skel.populate(infill, key);

		
		
	}

}
